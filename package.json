{
	"name": "stark",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"proto:generate": "npx pbjs --target static-module --wrap es6 --out src/lib/proto/generated/web_protocol_pb.js src/lib/proto/web_protocol.proto && npx pbts --out src/lib/proto/generated/web_protocol_pb.d.ts src/lib/proto/generated/web_protocol_pb.js",
		"proto:clean": "rm -rf src/lib/proto/generated || true",
		"generate-proto": "npm run proto:clean && mkdir -p src/lib/proto/generated && npm run proto:generate"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.8",
		"@rgossiaux/svelte-headlessui": "^2.0.0",
		"@tldraw/vec": "^1.9.2",
		"@use-gesture/vanilla": "^10.2.27",
		"buffer": "^6.0.3",
		"cbor-x": "^1.5.4",
		"firacode": "^6.2.0",
		"fontfaceobserver": "^2.3.0",
		"highlight.js": "^11.11.1",
		"lodash-es": "^4.17.21",
		"marked": "^15.0.12",
		"perfect-cursors": "^1.0.5",
		"protobufjs": "^7.5.0",
		"sshx-xterm": "5.2.1",
		"svelte": "^3.59.2",
		"svelte-feather-icons": "^4.0.1",
		"svelte-persisted-store": "^0.7.0",
		"xterm-addon-image": "^0.5.0",
		"xterm-addon-web-links": "^0.9.0",
		"xterm-addon-webgl": "^0.16.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.24.1",
		"@types/lodash-es": "^4.17.9",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"autoprefixer": "^10.4.15",
		"cssnano": "^6.0.1",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"protobufjs-cli": "^1.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "~5.2.2",
		"vite": "^4.4.9"
	}
}
